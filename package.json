{
  "name": "eslint-config-andrewaylett",
  "description": "Andrew's preferred ESLint configuration",
  "author": "Andrew Aylett",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewaylett/eslint-config-andrewaylett.git"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run buildonly",
    "buildonly": "tsc -b --verbose .",
    "test": "echo tested in postbuild",
    "postbuild": "NODE_OPTIONS=--experimental-vm-modules jest test/postbuild.test.ts",
    "prelint": "npm run buildonly",
    "lint": "eslint src test .eslintrc.cjs --ext .ts",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "lint:fix": "npm run lint -- --fix",
    "dev": "npm run lint:fix && npm run buildonly && npm run test",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/eslint": "8.4.6",
    "@types/jest": "28.1.8",
    "@types/node": "16.11.58",
    "@types/semver": "7.3.12",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-typescript-enum": "2.1.0",
    "extend-expect": "1.0.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "semver": "7.3.7",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.8.3"
  },
  "peerDependencies": {
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-eslint-comments": "^3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-jest-formatting": "^3",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-typescript-enum": "^2.1.0"
  }
}
